<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         version="3.1"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">

    <!-- Servlet definition -->
    <servlet>
        <servlet-name>AddSupplier</servlet-name>
        <servlet-class>supplier.AddSupplierServlet</servlet-class>
    </servlet>

    <!-- Servlet URL mapping -->
    <servlet-mapping>
        <servlet-name>AddSupplier</servlet-name>
        <url-pattern>/addSupplier</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditSupplier</servlet-name>
        <servlet-class>supplier.EditSupplierServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EditSupplier</servlet-name>
        <url-pattern>/editSupplier</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteSupplier</servlet-name>
        <servlet-class>supplier.DeleteSupplierServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteSupplier</servlet-name>
        <url-pattern>/deleteSupplier</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AddFeedbackServlet</servlet-name>
        <servlet-class>servlet.AddFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFeedbackServlet</servlet-name>
        <url-pattern>/addFeedback</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditFeedbackServlet</servlet-name>
        <servlet-class>servlet.EditFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditFeedbackServlet</servlet-name>
        <url-pattern>/editFeedback</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteFeedbackServlet</servlet-name>
        <servlet-class>servlet.DeleteFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteFeedbackServlet</servlet-name>
        <url-pattern>/deleteFeedback</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListFeedbackServlet</servlet-name>
        <servlet-class>servlet.ListFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListFeedbackServlet</servlet-name>
        <url-pattern>/listFeedback</url-pattern>
    </servlet-mapping>

</web-app>
